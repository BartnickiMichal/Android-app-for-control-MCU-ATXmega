<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SwitchActivity">

    <Button
        android:id="@+id/button_sw1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="45dp"
        android:layout_marginTop="143dp"
        android:text="SW0"
        tools:layout_editor_absoluteX="58dp"
        tools:layout_editor_absoluteY="133dp" />

    <Button
        android:id="@+id/button_sw2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="143dp"
        android:layout_marginEnd="68dp"
        android:text="SW1"
        tools:layout_editor_absoluteX="255dp"
        tools:layout_editor_absoluteY="128dp" />

    <Button
        android:id="@+id/button_sw3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="44dp"
        android:layout_marginBottom="320dp"
        android:text="SW2"
        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="278dp" />

    <Button
        android:id="@+id/button_sw4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="252dp"
        android:layout_marginBottom="320dp"
        android:text="SW3"
        tools:layout_editor_absoluteX="254dp"
        tools:layout_editor_absoluteY="283dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="19dp"
        android:layout_marginBottom="210dp"
        android:text="SW0: Powoduje poruszanie sie led w prawo strone" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="19dp"
        android:layout_marginBottom="170dp"
        android:text="SW1: Powoduje poruszanie sie led w lewo strone" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="19dp"
        android:layout_marginBottom="130dp"
        android:text="SW2: Zmniejsza czestotliwosc mrugania LED" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="19dp"
        android:layout_marginBottom="90dp"
        android:text="SW3: Zwieksza czetotliwosc mrugania LED" />
</RelativeLayout>